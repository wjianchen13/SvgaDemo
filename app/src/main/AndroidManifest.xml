<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SvgaApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SvgaDemo"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.SvgaActivity" />
        <activity android:name=".cache.CacheActivity" />
        <activity android:name=".cache.CacheActivity1" />
        <activity android:name=".cache.CacheActivity2" />
        <activity android:name=".cache.CacheActivity3" />
        <activity android:name=".webp.WebpActivity" />
        <activity android:name=".webp.WebpActivity1" />
        <activity android:name=".webp.WebpActivity2" />
        <activity android:name=".webp.WebpActivity3" />
        <activity android:name=".glide.GlideActivity" />
        <activity android:name=".glide.GlideActivity1" />
        <activity android:name=".compose.ComposeActivity" />
        <activity android:name=".compose.ComposeActivity1" />
        <activity android:name=".compose.ComposeActivity2" />

    </application>

</manifest>